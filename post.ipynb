    "import base64\n",
    "import requests\n",
    "with open(\"Part1.pdf\", \"rb\") as pdf:\n",
    "    encoded_string = base64.b64encode(pdf.read())\n",
    "    pdf.close()\n",
    "print(encoded_string[:10])\n",
    "r = requests.post('http://localhost:8000/sendfile/',data={'base64string':encoded_string},stream=True)\n",
    "print(r.text)  "
   ]
